{
    "manifest_version":2,
    "name": "Habernet",
    "description": "Get your news, in better way",
    "version":"1.0.0",
    "icons":{"64":"icon.ico"},
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"

    },
    "background":
    {
        "scripts": ["jquery-3.4.1.min.js","background.js"],
        "persistent": false
    },

    "permissions":["activeTab","tabs","cookies","<all_urls>", "webNavigation"]
}